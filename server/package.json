{
	"name": "server",
	"version": "1.0.0",
	"main": "src/index.ts",
	"license": "MIT",
	"scripts": {
		"dev": "nodemon -L src/index.ts"
	},
	"nodemonConfig": {
		"verbose": true,
		"ignore": [],
		"watch": [
			"src/**/*.ts"
		],
		"execMap": {
			"ts": "node --inspect=0.0.0.0:9229 --nolazy -r ts-node/register"
		}
	},
	"dependencies": {
		"dotenv": "^16.4.5",
		"express": "^4.19.2"
	},
	"devDependencies": {
		"@types/express": "^4.17.21",
		"@types/node": "^22.4.1",
		"nodemon": "^3.1.4",
		"ts-node": "^10.9.2",
		"typescript": "^5.5.4"
	}
}
